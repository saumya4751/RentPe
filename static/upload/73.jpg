       var date2 = new Date();
       var timeDiff =  date2.getTime() - date1.getTime();
       var mm = Math.floor(timeDiff / 1000 / 60);
 		if(!localStorage.getItem("setname") || !localStorage.getItem("timestart")){
				$('#myModal').modal('show'); // show
       }
       else if(mm <= 1 )
